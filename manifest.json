{
  "manifest_version": 2,
  "name": "SIDINGtex",
  "description": "Habilita el uso de MathJax en el SIDING",
  "version": "0.1",
  "web_accessible_resources": ["js/domManipulation.js"],
  "content_scripts": [
    {
      "matches": ["https://intrawww.ing.puc.cl/siding/*"],
      "js": ["js/main.js"]
    }
  ],
  "icons":
    { "16": "i/icon16.png",
      "48": "i/icon48.png",
      "128": "i/icon128.png",
      "256": "i/icon256.png"
    }
}
